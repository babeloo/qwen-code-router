{
  "default_config": [
    {
      "name": "openai-gpt4"
    }
  ],
  "configs": [
    {
      "config": [
        {
          "name": "openai-gpt4",
          "provider": "openai",
          "model": "gpt-4"
        },
        {
          "name": "deepseek-r1",
          "provider": "deepseek",
          "model": "deepseek-reasoner"
        }
      ]
    }
  ],
  "providers": [
    {
      "provider": "openai",
      "env": {
        "api_key": "YOUR_OPENAI_API_KEY",
        "base_url": "https://api.openai.com/v1",
        "models": [
          {
            "model": "gpt-4"
          },
          {
            "model": "gpt-3.5-turbo"
          }
        ]
      }
    },
    {
      "provider": "deepseek",
      "env": {
        "api_key": "YOUR_DEEPSEEK_API_KEY",
        "base_url": "https://api.deepseek.com",
        "models": [
          {
            "model": "deepseek-reasoner"
          },
          {
            "model": "deepseek-chat"
          }
        ]
      }
    }
  ]
}