{
  "name": "qwen-code-router",
  "version": "0.1.0",
  "description": "A command-line tool to manage and switch API configurations for different large model service providers when using Qwen Code",
  "main": "dist/index.js",
  "bin": {
    "qcr": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "build:release": "node scripts/build-release.js",
    "release": "npm run clean && npm run build && npm run build:release",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "keywords": [
    "qwen",
    "code",
    "router",
    "api",
    "configuration",
    "cli",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "jest-html-reporter": "^4.3.0",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.9",
    "jest": "^29.7.0",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}